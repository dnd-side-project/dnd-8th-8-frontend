{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn config:next && yarn browser && next dev",
    "build": "yarn config:next && next build",
    "start": "next start",
    "config:next": "tsc next.config.ts --skipLibCheck --module commonjs --target esnext --esModuleInterop true --allowSyntheticDefaultImports true --moduleResolution node",
    "browser": "start http://localhost:3000",
    "clean": "rm -rf yarn.lock .next node_modules",
    "reinstall": "yarn clean && yarn install",
    "lint": "next lint",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.2",
    "prettier-plugin-organize-imports": "^3.2.1"
  }
}
